{
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "avatar": {
            "type": "string"
          },
          "userId": {
            "type": "string",
            "readOnly": true
          },
          "username": {
            "type": "string",
            "maxLength": 150
          },
          "email": {
            "type": "string",
            "maxLength": 150
          },
          "name": {
            "type": "string",
            "maxLength": 150
          },
          "surname": {
            "type": "string",
            "maxLength": 150
          },
          "password": {
            "type": "string",
            "writeOnly": true,
            "maxLength": 128
          },
          "department": {
            "type": "string",
            "maxLength": 150
          },
          "jobTitle": {
            "type": "string",
            "maxLength": 150
          }
        },
        "required": [
          "id",
          "username"
        ]
      }
    }
  }
}